{
    "name": "reload/lumtorio-jira",

    "description": "Create Jira tickets for Lumturio issues",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "symfony/console": "^4.1",
        "stecman/symfony-console-completion": "^0.11.0",
        "reload/jira-security-issue": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "LumturioJira\\": "src/"
        }
    },
    "bin": [
        "lumturio-jira"
    ],
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/appocular/coding-standard"
      },
      {
        "type": "vcs",
        "url": "https://github.com/reload/jira-security-issue"
      }
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpstan/phpstan": "^0.12.5",
        "appocular/coding-standard": "^1.0"
    }
}
